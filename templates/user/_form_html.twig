<div class="auto-form">
    {{ form_errors(form) }}

    {{ form_start(form) }}

    {{ form_row(form.full_name) }}
    {{ form_row(form.role) }}
    {{ form_row(form.date_of_birth) }}
    {{ form_row(form.passport) }}

    {{ form_row(form.mobile_number) }}
    {{ form_row(form.email) }}

    <button class="btn btn-primary" type="submit">{{ 'Save' | trans }}</button>
    {{ form_end(form) }}
</div>
